import java.util.Scanner;
class Calculations{

public void add(int num1,int num2)
{
int add;
add=num1+num2;
System.out.println("Addition: " +add);
}

public void division(int num1,int num2)
{
  int div;
  div=num1/num2;
  System.out.println("Division is:"+div);
 }

public void subtraction(int num1,int num2)
{
  int sub;
  sub=num1-num2;
  System.out.println("Subtraction is:"+sub);
 }
  
 public void multiply(int num1,int num2)
 {
   int multiply;
   multiply=num1*num2;
   System.out.println("Multiply is : "+multiply);
  }
  
 public void max(int num1,int num2) {
 int max;
 if (num1<num2){
   max=num2;
 }
 else{
   max=num1;
 }returen max;
 
  System.out.println("Maximum Number is :" +max);
 }
 
 public static void main(String[]args){
 Scanner sc=new Scanner(System.in);
 System.out.println("Enter the No1 : ");
 int num1=sc.nextInt();
 System.out.println("Enter the No2 : ");
 int num2=sc.nextInt();
 
 Calculations a=new Calculations();
 Calculations b=new Calculations();
 Calculations c=new Calculations();
 Calculations d=new Calculations();
 Calculations e=new Calculations();
 
 a.add(num1,num2);
 b. division(num1,num2);
 c.subtraction(num1,num2);
 d.multiply(num1,num2);
 e.max(num1,num2);
 }
 }

 

 
 
 
 
 

